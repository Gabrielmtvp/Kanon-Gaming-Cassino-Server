generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model User {
    id       String   @id @default(uuid())
    name     String
    email    String
    password String
    Cassino  Cassino?
}

model Cassino {
    id     String @id @default(uuid())
    coins  Int
    userId String @unique

    user User @relation(fields: [userId], references: [id])
}
